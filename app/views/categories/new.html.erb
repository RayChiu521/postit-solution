<h4>Create a category</h4>

<div class="well">
	<%= render "shared/errors", obj: @category %>

	<%= form_for @category do |f| %>
	  <div class="control-group">
	  	<%= f.label :name %>
	  	<%= f.text_field :name %>
	  </div>
		<br/>

		<%= f.submit "Create Category", class: "btn btn-primary" %>
	<% end %>
</div>